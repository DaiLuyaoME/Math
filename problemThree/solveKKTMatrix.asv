function [v,w] = solveKKTMatrix(H,A,g,h)
L = choleskyDecomposition(H);
AT = A';
[m,n] = size(A);
opts1.LU = true;
opts2.UT = true;
%% step one
Y = zeros(n,m); % H^-1 * AT
for i = 1:m
    temp = linsolve(L,AT(:,i),opts1);
    temp = linsolve(L',temp,opts2);
    Y(:,i) = temp;
end

temp = linsolve(L,g,opts1);
y = linsolve(L',temp,opts2); % H^-1 * g

%% step two
S = -1 * A * Y;

%%



end